@model IEnumerable<OnlineDelivery.Models.Supplier>

@{
    ViewBag.Title = "Index";
}

<h2>Suppliers List</h2>


<table class="table">
    <tr>
        <th>
            Supplier Name
        </th>
        <th>
            Supplier Area
        </th>
        <th>
            Supplier Phone
        </th>
        <th>
            Action
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @item.Supplier_Name
        </td>
        <td>
            @item.Supplier_Area
        </td>
        <td>
            @item.Supplier_Phone
        </td>
       
        <td>
          
            <a href="/Productiteams/ProductList/@item.Supplier_ID">Details</a>
           
        </td>
    </tr>
}

</table>
